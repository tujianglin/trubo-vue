{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo-run dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky",
    "postinstall": "turbo run stub"
  },
  "devDependencies": {
    "@wm/eslint-config": "workspace:*",
    "@wm/typescript-config": "workspace:*",
    "@wm/turbo-run": "workspace:*",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@stylistic/stylelint-plugin": "^3.0.1",
    "@types/node": "^22.5.5",
    "cz-git": "^1.9.4",
    "husky": "^9.1.6",
    "less": "^4.2.0",
    "lint-staged": "^15.2.10",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "stylelint": "^16.9.0",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-recommended-less": "^3.0.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-prettier": "^5.0.2",
    "turbo": "^2.1.2",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "unocss": "^0.62.4"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=22"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./commitlint.config.mjs"
    }
  }
}
